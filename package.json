{
  "name": "lazyfill",
  "version": "1.5.5",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "npm version patch --no-git-tag-version && npm run build:exe",
    "build:minor": "npm version minor --no-git-tag-version && npm run build:exe",
    "build:major": "npm version major --no-git-tag-version && npm run build:exe",
    "build:exe": "electron-builder --win --x64"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Appule/LazyFill.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Appule/LazyFill/issues"
  },
  "homepage": "https://github.com/Appule/LazyFill#readme",
  "dependencies": {
    "express": "^5.2.1",
    "open": "^8.4.2"
  },
  "build": {
    "appId": "com.example.myapp",
    "win": {
      "target": "nsis",
      "icon": "public/assets/favicon.ico"
    },
    "files": [
      "main.js",
      "public/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "directories": {
      "output": "dist"
    }
  },
  "devDependencies": {
    "electron": "^39.2.7",
    "electron-builder": "^26.0.12"
  }
}
